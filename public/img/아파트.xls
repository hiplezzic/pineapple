function changetab(v){
	for(i=1;i<=2;i++){
		if(i==v){
		    document.getElementById("tabname"+i);	
			document.getElementById("tab"+i).style.display="";			
		}else{
			document.getElementById("tabname"+i);
			document.getElementById("tab"+i).style.display="none";
			
		}
	}
}


function toggleImage(obj)
{
	if(obj.src.indexOf("mc_7.jpg")>-1) obj.src = "../image/mn_7.jpg";
	else obj.src = "../image/mc_7.jpg";
}

$(document).ready(function() {

	$("#navi ul li div").hide(); 

	$("#navi>ul>li:lt(4)").hover(function ()
        {
			$(this).children("div").show();
        },function()
        {
            $(this).children("div").hide();
		});

	$("#navi>ul>li:eq(4)").click(function()
	{
		var div = $(this).children("div");
		
		div.toggle();
	});

	$("#navi .tap:eq(4) div").click(function()
	{
		toggleImage(document.getElementById("imgToggle"));
	});

});// document ready
</script>
