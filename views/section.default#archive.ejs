<div>Archive</div>
<br>
<div>
	<div class="row">
		<div class="column">No</div>
		<div class="column">Title</div>
		<div class="column">Image</div>
		<div class="column">Video</div>
	</div>
	<% for (var i=0; i<archive.length; i++) { %>
		<div class="row">
			<div class="column"><%- archive[i]['no'] %></div>
			<div class="column"><a href="<%- hrefAddress.post + archive[i]['no'] %>"><%- archive[i]['title'] %></a></div>
			<div class="column"><img src="<%- '/img/'+ JSON.parse(archive[i]['contents'])['img'] %>"></div>
			<div class="column"><iframe width="100" height="100" src="<%- JSON.parse(archive[i]['contents'])['videourl'].replace('watch?v=', 'embed/') %>" frameborder="0" allowfullscreen></iframe></div>

		</div>
	<% }; %>
</div>
